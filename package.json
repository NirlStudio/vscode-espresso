{
    "name": "vscode-espresso",
    "version": "0.10.6",
    "displayName": "Espresso Language (eslang)",
    "description": "The VS Code extension to support Espresso language.",
    "author": {
      "email": "leevi@nirlstudio.com",
      "name": "Leevi Li"
    },
    "publisher": "NirlStudio",
    "repository": "NirlStudio/vscode-espresso",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "espresso",
            "aliases": ["Espresso", "espresso", "espresso-lang", "eslang"],
            "extensions": [".es"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "espresso",
            "scopeName": "source.espresso",
            "path": "./syntaxes/espresso.tmLanguage.json"
        }]
    }
}