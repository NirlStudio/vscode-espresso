{
    "name": "sugly",
    "version": "0.10.4",
    "displayName": "Sugly",
    "description": "The VS Code extension to support Sugly language.",
    "author": {
      "email": "leevi@nirlstudio.com",
      "name": "Leevi Li"
    },
    "publisher": "NirlStudio",
    "repository": "NirlStudio/vscode-sugly",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "sugly",
            "aliases": ["Sugly", "sugly", "sugly-lang"],
            "extensions": [".s"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "sugly",
            "scopeName": "source.sugly",
            "path": "./syntaxes/sugly.tmLanguage.json"
        }]
    }
}